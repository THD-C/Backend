{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debuger jÄ™zyka Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/app",
        },
        {
            "name": "Debuger FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": ["main:app", "--reload"],
            "cwd": "${workspaceFolder}/app",
            "jinja": true
        }
    ]
}